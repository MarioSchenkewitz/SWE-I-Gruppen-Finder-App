<!DOCTYPE html>
<html class="use-all-space" lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Speet</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/general.css" />
</head>
<body class="masterbody use-all-space">
        <main class="use-all-space" role="main">
            @RenderBody()
        </main>

    @*uncomment when implemented
        <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2021 - Speet - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>*@

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
